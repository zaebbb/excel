{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "indent": "off",
    "arrow-parens": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "space-before-blocks": "off",
    "no-case-declarations":  "off",
    "one-var": "off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}